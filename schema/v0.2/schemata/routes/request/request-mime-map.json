{
  "$schema": "https://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "application/gzip": {
      "$ref": "request-content.json"
    },
    "application/json": {
      "$ref": "request-content.json"
    },
    "application/ld+json": {
      "$ref": "request-content.json"
    },
    "application/octet-stream": {
      "$ref": "request-content.json"
    },
    "application/xml": {
      "$ref": "request-content.json"
    },
    "text/csv": {
      "$ref": "request-content.json"
    },
    "text/html": {
      "$ref": "request-content.json"
    },
    "text/plain": {
      "$ref": "request-content.json"
    }
  },
  "patternProperties": {
    "^[a-z]+/[\\w\\-\\d.+]+$": {
      "$ref": "request-content.json"
    }
  },
  "minProperties": 1,
  "additionalProperties": false
}